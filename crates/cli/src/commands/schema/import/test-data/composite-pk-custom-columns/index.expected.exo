@postgres
module Database {
  @access(query=true, mutation=false)
  type Address {
    @pk city: String
    @pk state: String
    @pk street: String
    @pk zip: Int
    info: String?
    people: Set<Person>
  }

  @access(query=true, mutation=false)
  type Person {
    @pk firstName: String
    @pk lastName: String
    age: Int
    @column(mapping={city: "acity", state: "astate", street: "astreet", zip: "azip"}) address: Address?
  }
}