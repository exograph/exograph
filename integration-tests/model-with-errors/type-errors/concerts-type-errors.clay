@table("concerts")
model Concert {
  id: Int @pk @autoincrement
  title: Stringg @access(!self.id.bar || self.venue.name)
  venue: Venue @column("venueid")
}

@table("venues")
model Venue {
  id: Int @pk @autoincrement @access(Foo)
  name: String @range(min=-10, max=10)
  concerts: [Concert] @column("venueid")
}
