@table("concerts")
type Concert {
  id: Int @pk = autoincrement()
  @access(!self.id.bar | self.venue.name) title: Stringg 
  column("venueid") venue: Venue 
}

@postgres
service VenueService {
  @table("venues"
  type Venue {
    @access(Foo) @pk id: Int = autoincrement() 
    name: String
    @column("venueid") concerts: Concert> 
  }
}