{
  "openrpc": "1.3.2",
  "info": {
    "title": "Exograph RPC API",
    "version": "1.0.0"
  },
  "methods": [
    {
      "name": "get_videos",
      "description": "Get multiple `Video`s given the provided `where` filter, order by, limit, and offset",
      "params": [
        {
          "name": "where",
          "description": "Filter conditions for Videos",
          "required": false,
          "schema": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/VideoFilter"
              },
              {
                "type": "null"
              }
            ]
          }
        },
        {
          "name": "orderBy",
          "description": "Ordering for the results",
          "required": false,
          "schema": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/VideoOrdering"
              },
              {
                "type": "null"
              }
            ]
          }
        },
        {
          "name": "limit",
          "description": "Maximum number of results to return",
          "required": false,
          "schema": {
            "type": "integer",
            "nullable": true
          }
        },
        {
          "name": "offset",
          "description": "Number of results to skip",
          "required": false,
          "schema": {
            "type": "integer",
            "nullable": true
          }
        }
      ],
      "result": {
        "name": "result",
        "schema": {
          "type": "array",
          "items": {
            "$ref": "#/components/schemas/Video"
          }
        }
      }
    },
    {
      "name": "get_video",
      "description": "Get a single `Video` given primary key fields",
      "params": [
        {
          "name": "id",
          "description": "Primary key field: id",
          "required": true,
          "schema": {
            "type": "integer"
          }
        }
      ],
      "result": {
        "name": "result",
        "schema": {
          "oneOf": [
            {
              "$ref": "#/components/schemas/Video"
            },
            {
              "type": "null"
            }
          ]
        }
      }
    }
  ],
  "components": {
    "schemas": {
      "Video": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "slug": {
            "type": "string",
            "maxLength": 30
          },
          "nonce": {
            "type": "integer"
          },
          "views": {
            "type": "integer",
            "minimum": 0,
            "maximum": 9999999999
          },
          "filesize": {
            "type": "integer"
          }
        },
        "required": [
          "id",
          "name",
          "slug",
          "nonce",
          "views",
          "filesize"
        ]
      },
      "IntFilter": {
        "type": "object",
        "properties": {
          "eq": {
            "type": "integer",
            "nullable": true
          },
          "neq": {
            "type": "integer",
            "nullable": true
          },
          "lt": {
            "type": "integer",
            "nullable": true
          },
          "lte": {
            "type": "integer",
            "nullable": true
          },
          "gt": {
            "type": "integer",
            "nullable": true
          },
          "gte": {
            "type": "integer",
            "nullable": true
          }
        }
      },
      "StringFilter": {
        "type": "object",
        "properties": {
          "eq": {
            "type": "string",
            "nullable": true
          },
          "neq": {
            "type": "string",
            "nullable": true
          },
          "lt": {
            "type": "string",
            "nullable": true
          },
          "lte": {
            "type": "string",
            "nullable": true
          },
          "gt": {
            "type": "string",
            "nullable": true
          },
          "gte": {
            "type": "string",
            "nullable": true
          },
          "like": {
            "type": "string",
            "nullable": true
          },
          "ilike": {
            "type": "string",
            "nullable": true
          },
          "startsWith": {
            "type": "string",
            "nullable": true
          },
          "endsWith": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "VideoFilter": {
        "type": "object",
        "properties": {
          "id": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/IntFilter"
              },
              {
                "type": "null"
              }
            ]
          },
          "name": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/StringFilter"
              },
              {
                "type": "null"
              }
            ]
          },
          "slug": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/StringFilter"
              },
              {
                "type": "null"
              }
            ]
          },
          "nonce": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/IntFilter"
              },
              {
                "type": "null"
              }
            ]
          },
          "views": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/IntFilter"
              },
              {
                "type": "null"
              }
            ]
          },
          "filesize": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/IntFilter"
              },
              {
                "type": "null"
              }
            ]
          },
          "and": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/VideoFilter"
            },
            "nullable": true
          },
          "or": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/VideoFilter"
            },
            "nullable": true
          },
          "not": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/VideoFilter"
              },
              {
                "type": "null"
              }
            ]
          }
        }
      },
      "VideoOrdering": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "enum": [
              "ASC",
              "DESC"
            ],
            "nullable": true
          },
          "name": {
            "type": "string",
            "enum": [
              "ASC",
              "DESC"
            ],
            "nullable": true
          },
          "slug": {
            "type": "string",
            "enum": [
              "ASC",
              "DESC"
            ],
            "nullable": true
          },
          "nonce": {
            "type": "string",
            "enum": [
              "ASC",
              "DESC"
            ],
            "nullable": true
          },
          "views": {
            "type": "string",
            "enum": [
              "ASC",
              "DESC"
            ],
            "nullable": true
          },
          "filesize": {
            "type": "string",
            "enum": [
              "ASC",
              "DESC"
            ],
            "nullable": true
          }
        }
      }
    }
  }
}