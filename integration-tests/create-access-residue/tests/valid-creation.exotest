operation: |
  mutation($uId: Int!) {
      createIssue(data: {title: "I1", assignee: {id: $uId}}) {
          id
          title
          assignee {
            id
            name
            position
          }
      }
  }
variable: |
  {
    "uId": $.u1id
  }
auth: |
  {
    "sub": $.u1id
  }
response: |
  {
    "data": {
      "createIssue": {
        "id": 1,
        "title": "I1",
        "assignee": {
          "id": 1,
          "name": "U1-dev",
          "position": "developer"
        }
      }
    }
  }