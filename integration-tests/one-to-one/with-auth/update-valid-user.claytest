# User 2 trying to update own membership to point to another user
operation: |
  mutation {
    updateMembership(id: 1, data: {kind: "free", user: {id: 1}}) {
      id
      kind
      user {
        id
        name
      }
    }
  }
auth: |
  {
    "sub": 2,
    "role": "USER"
  }   
response: |
  {
    "data": {
      "updateMembership": null
    }
  }