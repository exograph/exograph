operation: |
  mutation {
    updateMembership(id: 1, data: {kind: "free", user: {id: 3}}) {
      id
      kind
      user {
        id
        name
      }
    }
  }
auth: |
  {
    "sub": 2,
    "role": "USER"
  }   
response: |
  {
    "errors": [
      {
        "message": "ERROR: duplicate key value violates unique constraint unique_constraint_user; DETAIL: Key (user_id)=(3) already exists."
      }
    ]
  }