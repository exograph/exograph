operation: |
    mutation($user_history: [String]!) {
        insertion: createChessPiece(data: {
            name: "rook",
            position_history: $user_history,
            neighbor_info: [
                ["none", "none", "none"],
                ["pawn", "rook", "none"],
                ["none", "none", "none"]
            ]
        }) {
            name
            position_history
            neighbor_info
        }
    }
variable: |
  {
    "user_history": ["Rh1", "Rh5", "Rd5"]
  }
response: |
    {
      "data": {
        "insertion": {
          "name": "rook",
          "position_history": [
            "Rh1",
            "Rh5",
            "Rd5"
          ],
          "neighbor_info": [
            [
              "none",
              "none",
              "none"
            ],
            [
              "pawn",
              "rook",
              "none"
            ],
            [
              "none",
              "none",
              "none"
            ]
          ]
        }
      }
    }