operation: |
    mutation {
        result: createImage(data: {
            name: "claytip",
            data: ""
        }) {
            id
            name
            data
        }
    }
response: |
    {
        "data": {
            "result": {
                "id": (actual) => {
                    let uuid_v4_regex = new RegExp("^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$");
                    return uuid_v4_regex.test(actual);
                },
                "name": "claytip",
                "data": ""
            }
        }
    }
