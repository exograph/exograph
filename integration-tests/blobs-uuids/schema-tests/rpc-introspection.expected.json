{
  "openrpc": "1.3.2",
  "info": {
    "title": "Exograph RPC API",
    "version": "1.0.0"
  },
  "methods": [
    {
      "name": "get_images",
      "description": "Get multiple `Image`s given the provided `where` filter, order by, limit, and offset",
      "params": [
        {
          "name": "where",
          "description": "Filter conditions for Images",
          "required": false,
          "schema": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/ImageFilter"
              },
              {
                "type": "null"
              }
            ]
          }
        },
        {
          "name": "orderBy",
          "description": "Ordering for the results",
          "required": false,
          "schema": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/ImageOrdering"
              },
              {
                "type": "null"
              }
            ]
          }
        },
        {
          "name": "limit",
          "description": "Maximum number of results to return",
          "required": false,
          "schema": {
            "type": "integer",
            "nullable": true
          }
        },
        {
          "name": "offset",
          "description": "Number of results to skip",
          "required": false,
          "schema": {
            "type": "integer",
            "nullable": true
          }
        }
      ],
      "result": {
        "name": "result",
        "schema": {
          "type": "array",
          "items": {
            "$ref": "#/components/schemas/Image"
          }
        }
      }
    },
    {
      "name": "get_image",
      "description": "Get a single `Image` given primary key fields",
      "params": [
        {
          "name": "id",
          "description": "Primary key field: id",
          "required": true,
          "schema": {
            "type": "string",
            "format": "uuid"
          }
        }
      ],
      "result": {
        "name": "result",
        "schema": {
          "oneOf": [
            {
              "$ref": "#/components/schemas/Image"
            },
            {
              "type": "null"
            }
          ]
        }
      }
    }
  ],
  "components": {
    "schemas": {
      "Image": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "data": {
            "type": "string",
            "format": "byte"
          }
        },
        "required": [
          "id",
          "name",
          "data"
        ]
      },
      "UuidFilter": {
        "type": "object",
        "properties": {
          "eq": {
            "type": "string",
            "nullable": true,
            "format": "uuid"
          },
          "neq": {
            "type": "string",
            "nullable": true,
            "format": "uuid"
          }
        }
      },
      "StringFilter": {
        "type": "object",
        "properties": {
          "eq": {
            "type": "string",
            "nullable": true
          },
          "neq": {
            "type": "string",
            "nullable": true
          },
          "lt": {
            "type": "string",
            "nullable": true
          },
          "lte": {
            "type": "string",
            "nullable": true
          },
          "gt": {
            "type": "string",
            "nullable": true
          },
          "gte": {
            "type": "string",
            "nullable": true
          },
          "like": {
            "type": "string",
            "nullable": true
          },
          "ilike": {
            "type": "string",
            "nullable": true
          },
          "startsWith": {
            "type": "string",
            "nullable": true
          },
          "endsWith": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "ImageFilter": {
        "type": "object",
        "properties": {
          "id": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/UuidFilter"
              },
              {
                "type": "null"
              }
            ]
          },
          "name": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/StringFilter"
              },
              {
                "type": "null"
              }
            ]
          },
          "data": {
            "type": "string",
            "nullable": true,
            "format": "byte"
          },
          "and": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ImageFilter"
            },
            "nullable": true
          },
          "or": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ImageFilter"
            },
            "nullable": true
          },
          "not": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/ImageFilter"
              },
              {
                "type": "null"
              }
            ]
          }
        }
      },
      "ImageOrdering": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "enum": [
              "ASC",
              "DESC"
            ],
            "nullable": true
          },
          "name": {
            "type": "string",
            "enum": [
              "ASC",
              "DESC"
            ],
            "nullable": true
          },
          "data": {
            "type": "string",
            "enum": [
              "ASC",
              "DESC"
            ],
            "nullable": true
          }
        }
      }
    }
  }
}