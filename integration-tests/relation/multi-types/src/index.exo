@postgres
module ConcertDatabase {
  @access(true)
  type Concert {
    @pk id: Int = autoIncrement()
    title: String
    @column("main_venue") mainVenue: Venue
    @column("alt_venue") altVenue: Venue?
  }

  @access(true)
  type Venue {
    @pk id: Int = autoIncrement()
    name: String
    @column("main_venue") mainConcerts: Set<Concert>?
    @column("alt_venue") altConcerts: Set<Concert>?
  }
}
