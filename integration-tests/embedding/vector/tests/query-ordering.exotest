operation: |
  fragment DocumentInfo on Document {
      id
      title
      content
      contentVector
  }
  query findDocs($searchVector: Vector!) {
      top: documents(orderBy: {contentVector: {value: $searchVector, order: ASC}}, limit: 1) {
          ...DocumentInfo
      }
      bottom: documents(orderBy: {contentVector: {value: $searchVector, order: DESC}}, limit: 1) {
          ...DocumentInfo
      }
      top2: documents(orderBy: {contentVector: {value: $searchVector, order: ASC}}, limit: 2) {
          ...DocumentInfo
      }
      bottom2: documents(orderBy: {contentVector: {value: $searchVector, order: DESC}}, limit: 2) {
          ...DocumentInfo
      }
      top3: documents(orderBy: {contentVector: {value: $searchVector, order: ASC}}, limit: 3) {
          ...DocumentInfo
      }
      bottom3: documents(orderBy: {contentVector: {value: $searchVector, order: DESC}}, limit: 3) {
          ...DocumentInfo
      }
      top4: documents(orderBy: {contentVector: {value: $searchVector, order: ASC}}, limit: 4) {
          ...DocumentInfo
      }
      bottom4: documents(orderBy: {contentVector: {value: $searchVector, order: DESC}}, limit: 4) {
          ...DocumentInfo
      }
      top5: documents(orderBy: {contentVector: {value: $searchVector, order: ASC}}, limit: 5) {
          ...DocumentInfo
      }
      bottom5: documents(orderBy: {contentVector: {value: $searchVector, order: DESC}}, limit: 5) {
          ...DocumentInfo
      }
  }
