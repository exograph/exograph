operation: |
  query {
      near: documents(where: {contentVector: {similar: {value: [0.95, 0.85, 0.15], distance: {lt: 0.5}}}}) @unordered {
          id
          title
          content
          contentVector
      }
      far: documents(where: {contentVector: {similar: {value: [0.95, 0.85, 0.15], distance: {gte: 0.5}}}}) @unordered {
          id
          title
          content
          contentVector
      }
  }
