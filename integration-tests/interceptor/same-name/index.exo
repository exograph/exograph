@deno("core.ts")
module Core {
  query getInt(): Int  
}

// The same interceptor is named same as in all modules.
// This allow testing that we apply the correct interceptor (considering both the module and the interceptor name).
@deno("intercept2.ts")
module Intercept2 {
  @around("query notExistingMethod")
  interceptor bypass()
}

@deno("intercept1.ts")
module Intercept1 {
  @around("query getInt")
  interceptor bypass()
}

@deno("intercept3.ts")
module Intercept3 {
  @around("query notExistingMethod")
  interceptor bypass()
}