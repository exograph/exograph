operation: |
  query {
    todo(id: 1) {
      id
      title
    }
    todo_id: todo(id: 1) {
      id
    }
    todo_title_aliased: todo(id: 1) {
      title_alias: title
    }
    getInfo {
      id
      title
    }
    info_id: getInfo {
      id
    }
    info_title_aliased: getInfo {
      id_alias: id
    }
  }
response: |
  {
    "data": {
      "todo": {
        "id": 1,
        "title": "Test"
      },
      "todo_id": {
        "id": 1
      },
      "todo_title_aliased": {
        "title_alias": "Test"
      },
      "getInfo": {
        "id": 1,
        "title": "Test"
      },
      "info_id": {
        "id": 1
      },
      "info_title_aliased": {
        "id_alias": 1
      }
    }
  }