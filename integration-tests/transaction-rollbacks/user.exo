@postgres
module UserPersistence {
    @table("user")
    @access(true)
    type User {
        @pk id: Int = autoIncrement()
        username: String
        email: String?
    }
}

@deno("user.ts")
module RegistrationModule {
    @access(true)
    export mutation registerUser(
        @inject exograph: Exograph,
        username: String,
        email: String
    ): Boolean
}