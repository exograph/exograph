@postgres
service PersonService {
  @access(true)
  type Person {
    id: Int = autoincrement() @pk
    name: String
  }
}

@deno("logger.js")
service Logger {
  type LogMessage {
    level: String
    message: String
  }

  export query logger(@inject claytip: Claytip, input: LogMessage): Boolean
  mutation debug(@inject claytip: Claytip, input: LogMessage): Boolean
}