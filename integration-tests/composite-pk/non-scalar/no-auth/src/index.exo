@postgres
module ChatModule {
  @access(true)
  type Chat {
    @pk id: Int = autoIncrement()
    title: String
    participants: Set<ChatParticipation>?
  }

  @access(true)
  type ChatParticipation {
    @pk chat: Chat
    @pk user: User
  }

  @access(true)
  type User {
    @pk id: Int = autoIncrement()
    name: String
    participatesIn: Set<ChatParticipation>?
  }
}
