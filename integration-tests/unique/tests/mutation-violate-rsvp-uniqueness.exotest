operation: |
    mutation($new_username: String!, $new_email_id: String!, $new_email_domain: String!) {
        createUser(data: {
            username: $new_username,
            primary_email_id: $new_email_id,
            email_domain: $new_email_domain
            rsvps: [ { event: "Concert2" }, { event: "Concert2" } ]
        }) {
            id
        }
    }
