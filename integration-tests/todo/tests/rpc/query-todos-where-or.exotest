# Test that where filter with OR logical operator works with RPC
operation:
  type: rpc
  payload: |
    {
      "method": "get_todos",
      "params": {
        "where": {"or": [{"priority": {"eq": "HIGH"}}, {"priority": {"eq": "LOW"}}]}
      }
    }
  unorderedPaths:
    - result
auth: |
  {
    "sub": null,
    "role": "admin"
  }
response: |
  {
    "result": [
      {"id": $.u1TodoIds[2], "title": "U1-T3", "completed": false, "priority": "HIGH"},
      {"id": $.u2TodoIds[2], "title": "U2-T3", "completed": false, "priority": "LOW"}
    ]
  }
