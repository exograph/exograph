import "mailing_list.clay"

model User {
    id: Int = autoincrement() @pk
    username: String
    subscribed_lists: Set<UserSubscription>? @column("subscribed_user_id")
}

model UserSubscription {
    id: Int = autoincrement() @pk
    user: User @column("subscribed_user_id")
    mailing_list: MailingList @column("subscribed_mailing_list")
}