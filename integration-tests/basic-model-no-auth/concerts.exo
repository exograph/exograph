@postgres
module ConcertModule {
  @table("concerts")
  @access(true)
  type Concert {
    @pk
    id: Int = autoIncrement();
    title: String
    @column("venueid") venue: Venue 
    published: Boolean
    @precision(20) @scale(2) price: Decimal 
  }

  @table("venues")
  @access(true)
  type Venue {
    @pk id: Int = autoIncrement() 
    name: String
    @column("venueid") concerts: Set<Concert>? 
    published: Boolean
    @size(4) latitude: Float 
  }
}
