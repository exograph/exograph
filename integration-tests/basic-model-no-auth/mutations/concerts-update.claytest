operation: |
    mutation {
      updateConcerts(where: {title: {eq: "Concert1"}}, data: {price: "50"}) {
        id
        price
        venue {
          id
          name
        }
      }
    }
response: |
    {
      "data": {
        "updateConcerts": [
          {
            "id": 1,
            "price": "50.00",
            "venue": {
              "id": 1,
              "name": "Venue1"
            }
          }
        ]
      }
    }
    