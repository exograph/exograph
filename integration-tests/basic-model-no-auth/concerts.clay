@postgres
service ConcertService {
  @table("concerts")
  @access(true)
  type Concert {
    id: Int = autoincrement() @pk
    title: String
    venue: Venue @column("venueid")
    published: Boolean
    price: Decimal @precision(20) @scale(2)
  }

  @table("venues")
  @access(true)
  type Venue {
    id: Int = autoincrement() @pk
    name: String
    concerts: Set<Concert>? @column("venueid")
    published: Boolean
    latitude: Float @size(4)
  }
}
