@postgres
service ConcertsService {
  @access(true)
  type Concert {
    @pk id: Int = autoIncrement()
    title: String
    venue: Venue
    published: Boolean
    @precision(20) @scale(2) price: Decimal
  }

  @access(true)
  type Venue {
    @pk id: Int = autoIncrement()
    name: String
    concerts: Set<Concert>?
    published: Boolean
    @size(4) latitude: Float
  }
}

