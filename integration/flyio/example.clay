@postgres
service ConcertsService {
  @table("concerts")
  type Concert {
    @pk id: Int = autoIncrement()
    title: String
    venue: Venue @column("venueid")
    published: Boolean
    price: Decimal @precision(20) @scale(2)
  }

  @table("venues")
  type Venue {
    @pk id: Int = autoIncrement()
    name: String
    concerts: Set<Concert>? @column("venueid")
    published: Boolean
    latitude: Float @size(4)
  }
}

