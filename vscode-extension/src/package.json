{
  "name": "claytip",
  "displayName": "Claytip",
  "description": "Syntax highlighting support for Claytip projects",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "claytip",
        "aliases": [
          "claytip",
          "claytip"
        ],
        "extensions": [
          ".clay"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "graphql",
        "extensions": [
          ".claytest"
        ]
      }
    ],
    "grammars": [
      {
        "language": "claytip",
        "scopeName": "source.clay",
        "path": "./syntaxes/claytip.tmLanguage.json"
      },
      {
        "injectTo": [
          "text.html.markdown"
        ],
        "scopeName": "markdown.clay.codeblock",
        "path": "./syntaxes/clay.markdown.codeblock.json",
        "embeddedLanguages": {
          "meta.embedded.block.clay": "clay"
        }
      }
    ]
  }
}
